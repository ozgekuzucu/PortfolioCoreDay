@model PortfolioCoreDay.Entities.Contact
@{
    ViewData["Title"] = "UpdateContact";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h1>İletişim Sayfası Güncelleme Girişi</h1>

<form method="post">
    <input type="hidden" asp-for="Id" />
    <label>Adres</label>
    <input type="text" asp-for="Address" class="form-control" style="border:1px solid black">
    <br />
    <label>Mail</label>
    <input type="text" asp-for="Mail" class="form-control" style="border:1px solid black">
    <br />
    <label>Telefon</label>
    <input type="text" asp-for="Phone" class="form-control" style="border:1px solid black">
    <br />
    <label>Harita Url</label>
    <input type="text" asp-for="MapUrl" class="form-control" style="border:1px solid black">
    <br />
    <button class="btn btn-primary">İletişim Sayfası Bilgisi Güncelle</button>
</form>