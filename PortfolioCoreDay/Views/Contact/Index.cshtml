@model List<Contact>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}


<h2>İletişim İşlemleri</h2>

<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>Adres</th>
        <th>Mail</th>
        <th>Telefon</th>
        <th>Harita Url</th>
        <th>Güncelle</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Address</td>
            <td>@item.Mail</td>
            <td>@item.Phone</td>
            <td>@item.MapUrl</td>
            <td><a href="/Contact/UpdateContact/@item.Id" class="btn btn-success">Güncelle</a></td>
        </tr>
    }
</table>