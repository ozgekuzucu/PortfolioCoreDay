@model List<Contact>
<section id="contact" class="contact section">

    <!-- Section Title -->
    <div class="container section-title" data-aos="fade-up">
        <h2>İletişim</h2>
        <p>Her türlü proje, iş birliği ya da sorularınız için benimle iletişime geçmekten çekinmeyin. Size en kısa sürede geri dönüş yapacağım.</p>
    </div><!-- End Section Title -->

    <div class="container" data-aos="fade-up" data-aos-delay="100">

        <div class="row gy-4">

            @await Component.InvokeAsync("_DefaultContactMeComponentPartial")

            @await Component.InvokeAsync("_DefaultMessageComponentPartial")
        </div>

    </div>

</section><!-- /Contact Section -->