@model List<Feature>
<section id="hero" class="hero section dark-background">
    @foreach (var item in Model)
    {
        <img src="@item.ImageUrl2" alt="" data-aos="fade-in" class="">

        <div class="container" data-aos="fade-up" data-aos-delay="100">
            <h2>@item.NameSurname</h2>

            <p>Çalışma Alanlarım: <span class="typed" data-typed-items="@item.WorkTitle1, @item.WorkTitle2, @item.WorkTitle3,@item.WorkTitle4"></span><span class="typed-cursor typed-cursor--blink" aria-hidden="true"></span></p>

        </div>
    }
</section><!-- /Hero Section -->