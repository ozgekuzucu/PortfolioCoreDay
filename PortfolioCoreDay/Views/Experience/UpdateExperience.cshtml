@model PortfolioCoreDay.Entities.Experience
@{
    ViewData["Title"] = "UpdateExperience";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}


<h1>Deneyim Güncelleme Girişi</h1>

<form method="post">
    <input type="hidden" asp-for="ExperienceId" />
    <label>Şirket</label>
    <input type="text" asp-for="CompanyName" class="form-control" style="border:1px solid black">
    <br />
    <label>Ünvan</label>
    <input type="text" asp-for="Title" class="form-control" style="border:1px solid black">
    <br />
    <label>Açıklama</label>
    <input type="text" asp-for="Description" class="form-control" style="border:1px solid black">
    <br />
    <label>Tarih</label>
    <input type="text" asp-for="ExperienceDate" class="form-control" style="border:1px solid black">
    <br />
    <button class="btn btn-primary">Deneyimi Güncelle</button>
</form>

